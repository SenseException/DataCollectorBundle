{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <img width="13" height="28" src="data:image/png;base64,
" alt="Example">
        Example
    {% endset %}
    {% set text %}
        <b>Your IP:</b> {{ collector.ip }}
    {% endset %}
    
    {# helper template for link to profiler page (block: panel) #}
    {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block head %}
    {# JS or CSS files #}
    {{ parent() }}
{% endblock %}

{% block menu %}
    <span class="label">
        <span class="icon"><img src="data:image/png;base64," alt="Example" /></span>
        <strong>Profiler Example (IP)</strong>
    </span>
{% endblock %}

{% block panel %}
    <h2>Profiler Example</h2>
    <h3>Your IP info</h3>
    
    <p><strong>{{ collector.ip }}</strong></p>
    
    <p>Usually, it shows the localhost IP on dev... which makes this profiler info totally useless.</p>
{% endblock %}
